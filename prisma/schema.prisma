// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model carrots {
 id Int @id @default(autoincrement())
 userid String
 word String
 append Int
 available Boolean
 business Boolean
 pro Boolean
 premium Boolean
 banned Boolean
 sold Boolean
 price Int
 cid String
 ipfs String
 cidw String
 sitepublickey String
 siteprivatekey String
 publickey String
 privkey String
 timestamp String
 updateinfo String
}

model plans {
 id Int @id @default(autoincrement())
 level Int
 name String
 type String
 benefits String
}

model user {
 id Int @id @default(autoincrement())
 fkword Int
 fkwallet Int
 fkbanned Int
 firstname String
 lastname String
 username String
 caret String
 avatar String
 password String
 plan Int
 admin Boolean
 level Int
 email String
 chain String
 account String
 fkword2 Int
 caret2 String
 chain2 String
 account2 String
 fkword3 Int
 caret3 String
 chain3 String
 account3 String
 join_date String
 dateupdated String
} 

model wallet {
 id Int @id @default(autoincrement())
 userid Int
 primarywallet Int
 provider String
 chain String
 wallet String
}

model banned {
 id Int @id @default(autoincrement())
 ip Int
 count Int
 ban Int
}

model promo {
 id Int @id @default(autoincrement())
 code String
 discount String
 expire String
 }

model sales {
 id Int @id @default(autoincrement())
 new Int
 addon Int
 email String
 word String
 price Int
 plan Int
 referral String
 region String
 promo String
 salesdate String
}